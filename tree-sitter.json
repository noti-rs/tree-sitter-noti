{
  "grammars": [
    {
      "name": "noti",
      "camelcase": "Noti",
      "scope": "source.noti",
      "path": ".",
      "file-types": [
        "noti"
      ],
      "injection-regex": "^noti$",
      "highlights": [
        "queries/highlights.scm"
      ]
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Noti grammar for tree-sitter",
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-noti"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
